<div>
	<%= form_for(@order) do |f| %>
	<div class="form-group">
	 <label>依頼タイトル</label>
	 <%= f.text_field :order_title %>
	</div>
	<div class="form-group">
	 <label>内容</label>
	 <%= f.text_field :order_text %>
	</div>
	<div class="form-group">
	 <label>イメージ</label>
	 <%= f.attachment_field :order_image %>
	</div>
	<div class="form-group">
	 <label>締切日</label>
	 <%= f.date_select :deadline %>
	</div>

	<% end %>
</div>